# Telegram Bot Token - получите от @BotFather
BOT_TOKEN=your_bot_token_here

# URL вашего Mini App (замените на реальный домен)
WEBAPP_URL=https://your-domain.com

# Порт для webhook (если используете)
PORT=3000

# Режим разработки
NODE_ENV=development

# Webhook URL (если используете webhook вместо polling)
WEBHOOK_URL=https://your-domain.com/webhook

# Секретный ключ для webhook (опционально)
WEBHOOK_SECRET=your_webhook_secret 